<h1>NEW QUESTION</h1>


<h3>Add a question:</h3>
<%= form_for @question, :html => {multipart: true} do |f|%>

  <%= f.label :title %>:
  <%= f.text_field :title %><br>

  <%= f.label :description %>:<br>
  <%= f.text_area :description %><br>

  <%= f.file_field :image %><br>

  <%= f.submit %>
<% end %>
